<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="ManageUsers.aspx">
    <system.web>
        <authorization>
          <allow roles="Administradores"/>
          <deny users ="*"/>
        </authorization>
    </system.web>
  </location>
  <location path="Login.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
</configuration>
